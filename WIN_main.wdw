      Procedure ChangeNation(Nation_Num)
      IF Nation_Num <> Nation() THEN
      	Nation(Nation_Num)
      	// Titre de la fenêtre - window title
      	MyWindow..Title = <§$0007§>
      	// Change le texte des libellés - Changes the text of captions
      	STC_LANG..Caption = <§$0000§>
      	STC_Title..Caption = <§$0001§>
      	// Change le texte central - Changes the main text
      	STC_TEXTAREA..Caption = MessTranslate(Val(STC_TEXTAREA..Note))
      	// Change les libellés des boutons - change button captions
      	ChangeButtonCap(Nation_Num)
      END
      
     type : 458752
   -
     name : ClickMenu
     internal_properties : 
     procedure_id : 1220150275525122843
     type_code : 14
     code : |1+
      Procedure ClickMenu(num)
      // num = 0 	displays the menu in its current status
      // num = 0 	displays the menu in its current status
      // num > 0 	modifies the status of the menu...
      //			and displays the menu with the sub-menu opened or closed according to the value of menustatus
      // num > 0 	modifies the status of the menu...
      // 			and displays the menu with the submenu opened or closed according to the value of menustatus
      
      // variables locales - local variables
      nActiveNation is int = Nation
      previouslyActiveMenu is int
      newActiveMenu is int
      previouslyActiveMenu = Position(MenuStatus, “1”)
      currentXPos is int
      currentYPos is int
      MenuSize is int = Length(MenuStatus)
      
      // MenuStatus is modified
      // MenuStatus is modified
      
      IF num > 0 THEN
      	IF previouslyActiveMenu = num THEN
      		// juste une RAZ du menu
      		// reset the menu
      		MenuStatus = RepeatString(“0”, MenuSize)
      	ELSE
      		// disables the previous menu
      		// disables the previous menu
      		IF previouslyActiveMenu > 0 THEN
      			MenuStatus = Left(MenuStatus, previouslyActiveMenu - 1) + “0”
      		END
      		MenuStatus += Right(MenuStatus, MenuSize - previouslyActiveMenu)
      		// activate le nouveau menu
      		// enables the new menu
      		MenuStatus = Left(MenuStatus, num - 1) + “1”
      		MenuStatus += Right(MenuStatus, MenuSize - num)
      	END
      END
      
      // Affiche les boutons en fonction de MenuStatus
      // Displays the buttons according to MenuStatus
      
      // Tous les boutons invisibles
      // All the buttons are made invisible
      Obj is object cNode dynamic
      Bt is object cButton dynamic
      Obj = Menu:m_clFirstNode
      
      IF Obj <> Null THEN
      	Bt = Obj:m_pcldataNode
      	{Bt:m_sNameOfControl,indControl}..Visible = False
      	WHILE Obj:m_pclnextNode <> Null
      		Obj = Obj:m_pclnextNode	
      		Bt = Obj:m_pcldataNode
      		{Bt:m_sNameOfControl,indControl}..Visible = False
      	END
      END
      
      // Place les boutons et les rend visibles
      // en fonction de MenuStatus
      // Positions the buttons and makes them visible
      // according to menustatus
      
      newActiveMenu = Position(MenuStatus, “1”)
      
      Obj = Menu:m_clFirstNode
      
      IF Obj <> Null THEN
      	Bt = Obj:m_pcldataNode
      	// The first button is always visible
      	// The first button is always visible
      	{Bt:m_sNameOfControl,indControl}..Visible = True
      	currentXPos = gnInitXPos
      	currentYPos = gnInitYPos
      	
      	WHILE Obj:m_pclnextNode <> Null
      		Obj = Obj:m_pclnextNode	
      		Bt = Obj:m_pcldataNode
      		IF Bt:m_nButtonType >= 0 THEN
      			// This button is displayed at a new position
      			// This button is displayed at a new position
      			currentYPos += gnIncrementY
      			{Bt:m_sNameOfControl,indControl}..X = currentXPos
      			{Bt:m_sNameOfControl,indControl}..Y = currentYPos
      			{Bt:m_sNameOfControl,indControl}..Visible = True
      		ELSE
      			IF newActiveMenu > 0 THEN
      				IF Abs(Bt:m_nButtonType) = newActiveMenu THEN
      					currentYPos += gnIncrementY
      					{Bt:m_sNameOfControl,indControl}..X = currentXPos
      					{Bt:m_sNameOfControl,indControl}..Y = currentYPos
      					{Bt:m_sNameOfControl,indControl}..Visible = True
      				END
      			END
      		END
      	END // tant que - while
      END
