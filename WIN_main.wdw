     procedure_id : 1220150275525188379
     type_code : 14
     code : |1+
      Procedure ChangeButtonCap(nNation)
      // Tous les boutons sont rendus invisibles
      // All the buttons are made invisible
      Obj is object cNode dynamic
      Bt is object cButton dynamic
      Obj = Menu:m_clFirstNode
      
      IF Obj <> Null THEN
      	Bt = Obj:m_pcldataNode
      	IF nNation = _FRENCH_ THEN 
      		{Bt:m_sNameOfControl,indControl}..Caption = Bt:m_sCaptionFR
      	ELSE
      		{Bt:m_sNameOfControl,indControl}..Caption = Bt:m_sCaptionUS
      	END
      	WHILE Obj:m_pclnextNode <> Null
      		Obj = Obj:m_pclnextNode	
      		Bt = Obj:m_pcldataNode
      		IF nNation = _FRENCH_ THEN 
      			{Bt:m_sNameOfControl,indControl}..Caption = Bt:m_sCaptionFR
      		ELSE
      			{Bt:m_sNameOfControl,indControl}..Caption = Bt:m_sCaptionUS
      		END
      	END
      END
      
      
